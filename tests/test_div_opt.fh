# Test specialized DIV opcodes
fn main() {
    # Integer division
    let a = 10;
    let b = 3;
    let result_int = a / b;
    printf("Int div: %d / %d = %d\n", a, b, result_int);

    # Float division
    let x = 10.0;
    let y = 3.0;
    let result_float = x / y;
    printf("Float div: %f / %f = %f\n", x, y, result_float);

    # Mixed division
    let mixed = a / y;
    printf("Mixed div: %d / %f = %f\n", a, y, mixed);

    # Division by zero check
    let zero = 0;
    # This should error: let bad = a / zero;

    printf("All division tests passed!\n");
}
