
fn make_counter(num) {
    return {
        "next" : fn() {
            num = num + 1;
        },

        "read" : fn() {
            return num;
        },
    };
}

fn main() {
    print("Hello, world!\n");

    let c1 = make_counter(0);
    let c2 = make_counter(10);
    c1.next();
    c2.next();
    printf("%d, %d\n", c1.read(), c2.read());    # prints 1, 11

    c1.next();
