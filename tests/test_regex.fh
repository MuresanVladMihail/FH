#-
    . Dot, matches any character
    ^ Start anchor, matches beginning of string
    $ End anchor, matches end of string
    * Asterisk, match zero or more (greedy)
    + Plus, match one or more (greedy)
    ? Question, match zero or one (non-greedy)
    [abc] Character class, match if one of {'a', 'b', 'c'}
    [^abc] Inverted class, match if NOT one of {'a', 'b', 'c'} NOTE: This feature is currently broken for some usage of character ranges!
    [a-zA-Z] Character ranges, the character set of the ranges { a-z | A-Z }
    \s Whitespace, \t \f \r \n \v and spaces
    \S Non-whitespace
    \w Alphanumeric, [a-zA-Z0-9_]
    \W Non-alphanumeric
    \d Digits, [0-9]
    \D Non-digits
-#
fn main() {
    let STR = "Hello type=candy ! type bye";
    assert(string_match(STR, '^Hello') == 0);
    assert(string_match(STR, 'bye$') == 24);
    assert(string_match(STR, '.type=candy') == 5);
    assert(string_match(STR, '[!?#]') == 17);
    assert(string_match(STR, '[^candy !]') == 0);
    assert(string_match(STR, '.FH') == null);
    print('ok\n');
}
