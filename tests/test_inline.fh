# Simple function that should be inlined
fn add_simple(a, b) {
    return a + b;
}

fn main() {
    let sum = 0;
    let start = os_time();

    # Call many times to see inlining effect
    for (let i = 0; i < 1000000; i++) {
        sum = sum + add_simple(i, 1);
    }

    let end = os_time();
    let diff = os_difftime(start, end);

    printf("Sum: %d in %d ms\n", sum, diff);
}
