
fn main(args)
{
    let x = {
        "x" : "x",
        "y" : {
            "z" : "z",
            "f" : fn(x) { return x; },
        },
        "g" : fn(x) { return x + 1; },
    };

    if (x.x != "x") error("nope");
    if (x.y.z != "z") error("nope");
    if (x.y["z"] != "z") error("nope");
    if (x["y"].z != "z") error("nope");
    if (x["y"]["z"] != "z") error("nope");
    if (x.g(1) != 2) error("nope");
    if (x.y.f(42) != 42) error("nope");

    print("ok\n");
}

